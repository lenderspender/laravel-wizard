{
  "name": "lenderspender/laravel-wizard",
  "description": "Control web steps using a wizard.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Bert van Hoekelen",
      "email": "bert@magwel.nl"
    }
  ],
  "autoload": {
    "psr-4": {
      "LenderSpender\\LaravelWizard\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LenderSpender\\LaravelWizard\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4 | ^8.0",
    "laravel/framework": "^7.4 | ^8.0"
  },
  "require-dev": {
    "lenderspender/php-cs-fixer-rules": "dev-master",
    "mockery/mockery": "^1.3.3",
    "orchestra/testbench": "^5.3",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan": "^0.12.14",
    "phpstan/phpstan-mockery": "^0.12.3",
    "phpunit/phpunit": "^8.5 || ^9.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
      ]
    }
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse",
    "fix": "vendor/bin/php-cs-fixer fix",
    "test": "vendor/bin/phpunit"
  }
}
